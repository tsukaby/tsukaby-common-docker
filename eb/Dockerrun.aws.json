{
  "AWSEBDockerrunVersion": 2,
  "volumes": [],
  "containerDefinitions": [
    {
      "name": "tech-blog",
      "image": "quay.io/tsukaby/tech-blog",
      "essential": true,
      "memory": 256
    },
    {
      "name": "anime-lineup",
      "image": "tsukaby/anime-lineup-docker",
      "essential": true,
      "memory": 512
    },
    {
      "name": "nginx-proxy",
      "image": "nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "tech-blog",
        "anime-lineup"
      ],
      "mountPoints": []
    }
  ]
}